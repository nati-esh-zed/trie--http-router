{
  "name": "@trie/http-router",
  "version": "0.4.4",
  "license": "MIT",
  "author": "Natnael",
  "description": "A lightweight, and lightning fast http router library for Deno #server",
  "exports": {
    ".": "./mod.ts",
    "./jwt": "./src/jwt.ts",
    "./engines/render": "./src/engines/render/index.ts"
  },
  "nodeModulesDir": "auto",
  "fmt": {
    "include": ["src/", "test/"],
    "exclude": ["dist/"],
    "options": {
      "lineWidth": 80,
      "indentWidth": 2,
      "semiColons": true,
      "useTabs": false,
      "singleQuote": false,
      "proseWrap": "always"
    }
  },
  "lint": {
    "include": ["src/"],
    "exclude": ["dist/"]
  },
  "compilerOptions": {
    "lib": ["dom", "deno.ns"],
    "strict": true
  }
}
